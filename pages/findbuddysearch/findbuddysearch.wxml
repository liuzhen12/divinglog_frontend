<view class="page">
    <view class="page__hd">
        <view class="page__title"></view>
        <view class="page__desc"></view>
    </view>
    <view class="page__bd page__bd_spacing">
        <view class="weui-cells box">
            <view class="weui-cell weui-cell_access">
                <view class="section">
                    <picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
                        <view class="picker">
                        开始日期: {{date}}
                        </view>
                    </picker>
                </view>
            </view>
            <view class="weui-cell weui-cell_access">
                <view class="section">
                    <picker mode="date" value="{{date}}" start="2015-09-01" end="2017-09-01" bindchange="bindDateChange">
                        <view class="picker">
                        结束日期: {{date}}
                        </view>
                    </picker>
                </view>
            </view>
            <view class="weui-cell weui-cell_access">
                <view class="weui-cell__bd">{{'潜点'}}</view>
                <view class="weui-cell__ft">Bali</view>
            </view>
            <view class="weui-cell weui-cell_access">
                <view class="section">
                    <picker bindchange="bindPickerChange" value="{{index}}" range="{{array}}">
                        <view class="picker">
                            住宿：{{array[index]}}
                        </view>
                    </picker>
                </view>
            </view>
        </view>
        <view class="weui-cells box">
            <navigator url="../findbuddylist/findbuddylist" class="weui_btn weui_btn_default" hover-class="changestyle">
                <button class="weui_btn weui_btn_default">搜索</button>
            </navigator>
        </view>
    </view>
</view>