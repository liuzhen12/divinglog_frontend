<!--pages/registerrole/registerrole.wxml-->
<view class="page">
    <view class="page__hd">
        <view class="page__title"></view>
        <view class="page__desc"></view>
    </view>
    <view class="page__bd page__bd_spacing">
        <view class="weui_cells_title">Please choose your role:</view>
        <view class="weui-cells box">
          <navigator url="../registerdiver/registerdiver" class="weui-cell weui-cell_access" hover-class="changestyle">
                <view class="weui-cell__bd">{{'Diver'}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
          <navigator url="../registercoach/registercoach" class="weui-cell weui-cell_access" hover-class="changestyle">
                <view class="weui-cell__bd">{{'Instructor'}}</view>
                <view class="weui-cell__ft weui-cell__ft_in-access"></view>
          </navigator>
        </view>
    </view>
</view>
