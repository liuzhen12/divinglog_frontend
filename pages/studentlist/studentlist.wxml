<view class="page">
    <view class="page__hd">
        <view class="page__title"></view>
        <view class="page__desc"></view>
    </view>
    <view class="weui-cells">
            <view class="weui-cell" wx:for="{{student}}" wx:key="id">
                <view class="weui-cell__hd">
                    <image src="{{item.avatar_url}}" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                </view>
                <view class="weui-cell__bd">{{item.nick_name}}</view>
                <view class="weui-cell__ft"></view>
            </view>
        </view>
</view>